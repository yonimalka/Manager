<svg viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <!-- "m" shaped growth chart with 3 bars - animated once -->
  <defs>
    <style>
      @keyframes growLeft {
        0% { transform: scaleY(0); transform-origin: bottom; opacity: 0; }
        60% { transform: scaleY(1.1); transform-origin: bottom; opacity: 1; }
        100% { transform: scaleY(1); transform-origin: bottom; opacity: 1; }
      }
      
      @keyframes growMiddle {
        0% { transform: scaleY(0); transform-origin: bottom; opacity: 0; }
        60% { transform: scaleY(1.15); transform-origin: bottom; opacity: 1; }
        100% { transform: scaleY(1); transform-origin: bottom; opacity: 1; }
      }
      
      @keyframes growRight {
        0% { transform: scaleY(0); transform-origin: bottom; opacity: 0; }
        60% { transform: scaleY(1.1); transform-origin: bottom; opacity: 1; }
        100% { transform: scaleY(1); transform-origin: bottom; opacity: 1; }
      }
      
      .bar-left {
        animation: growLeft 0.8s ease-out forwards;
        animation-delay: 0s;
      }
      
      .bar-middle {
        animation: growMiddle 0.8s ease-out forwards;
        animation-delay: 0.15s;
      }
      
      .bar-right {
        animation: growRight 0.8s ease-out forwards;
        animation-delay: 0.3s;
      }
    </style>
  </defs>
  
  <g transform="translate(100, 100)">
    <!-- Left peak (left hump of m) -->
    <rect class="bar-left" x="-45" y="-45" width="22" height="70" rx="3" fill="#0A7AFF"/>
    
    <!-- Middle valley -->
    <rect class="bar-middle" x="-11" y="-25" width="22" height="50" rx="3" fill="#0A7AFF"/>
    
    <!-- Right peak (right hump of m) -->
    <rect class="bar-right" x="23" y="-45" width="22" height="70" rx="3" fill="#0A7AFF"/>
  </g>
</svg>